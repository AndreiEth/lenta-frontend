{
  "name": "lenta-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "lint": "eslint .",
    "type-check": "tsc"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.11",
    "@mui/material": "^5.14.10",
    "@mui/x-charts": "^6.0.0-alpha.14",
    "@mui/x-data-grid": "^6.15.0",
    "@reduxjs/toolkit": "^1.8.1",
    "@types/axios": "^0.14.0",
    "axios": "^1.5.1",
    "base-ui": "^0.0.291",
    "baseui": "^13.0.0",
    "classnames": "^2.3.2",
    "material-react-table": "^1.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.16.0",
    "react-select": "^5.7.7"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.2.5",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jsdom": "^21.1.0",
    "prettier": "^2.7.1",
    "prettier-config-nick": "^1.0.2",
    "typescript": "^5.0.2",
    "vite": "^4.0.0",
    "vitest": "^0.30.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "quotes": [
        "warn",
        "double"
      ],
      "jsx-quotes": [
        "warn",
        "prefer-double"
      ],
   
      "react/jsx-no-target-blank": "off",
      "linebreak-style": [
        "error",
        "windows"
      ]
    }
  },
  "prettier": "prettier-config-nick"
}
